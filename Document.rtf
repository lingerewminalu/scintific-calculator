    //eval function\par
    public static double eval(final String str) \{\par
        return new Object() \{\par
            int pos = -1, ch;\par
\par
            void nextChar() \{\par
                ch = (++pos < str.length()) ? str.charAt(pos) : -1;\par
            \}\par
\par
            boolean eat(int charToEat) \{\par
                while (ch == ' ') nextChar();\par
                if (ch == charToEat) \{\par
                    nextChar();\par
                    return true;\par
                \}\par
                return false;\par
            \}\par
\par
            double parse() \{\par
                nextChar();\par
                double x = parseExpression();\par
                if (pos < str.length()) throw new RuntimeException("Unexpected: " + (char)ch);\par
                return x;\par
            \}\par
\par
            // Grammar:\par
            // expression = term | expression `+` term | expression `-` term\par
            // term = factor | term `*` factor | term `/` factor\par
            // factor = `+` factor | `-` factor | `(` expression `)`\par
            //        | number | functionName factor | factor `^` factor\par
\par
            double parseExpression() \{\par
                double x = parseTerm();\par
                for (;;) \{\par
                    if      (eat('+')) x += parseTerm(); // addition\par
                    else if (eat('-')) x -= parseTerm(); // subtraction\par
                    else return x;\par
                \}\par
            \}\par
\par
            double parseTerm() \{\par
                double x = parseFactor();\par
                for (;;) \{\par
                    if      (eat('*')) x *= parseFactor(); // multiplication\par
                    else if (eat('/')) x /= parseFactor(); // division\par
                    else return x;\par
                \}\par
            \}\par
\par
            double parseFactor() \{\par
                if (eat('+')) return parseFactor(); // unary plus\par
                if (eat('-')) return -parseFactor(); // unary minus\par
\par
                double x;\par
                int startPos = this.pos;\par
                if (eat('(')) \{ // parentheses\par
                    x = parseExpression();\par
                    eat(')');\par
                \} else if ((ch >= '0' && ch <= '9') || ch == '.') \{ // numbers\par
                    while ((ch >= '0' && ch <= '9') || ch == '.') nextChar();\par
                    x = Double.parseDouble(str.substring(startPos, this.pos));\par
                \} else if (ch >= 'a' && ch <= 'z') \{ // functions\par
                    while (ch >= 'a' && ch <= 'z') nextChar();\par
                    String func = str.substring(startPos, this.pos);\par
                    x = parseFactor();\par
                    if (func.equals("sqrt")) x = Math.sqrt(x);\par
                    else if (func.equals("sin")) x = Math.sin(Math.toRadians(x));\par
                    else if (func.equals("cos")) x = Math.cos(Math.toRadians(x));\par
                    else if (func.equals("tan")) x = Math.tan(Math.toRadians(x));\par
                    else if (func.equals("log")) x = Math.log10(x);\par
                    else if (func.equals("ln")) x = Math.log(x);\par
                    else throw new RuntimeException("Unknown function: " + func);\par
                \} else \{\par
                    throw new RuntimeException("Unexpected: " + (char)ch);\par
                \}\par
\par
                if (eat('^')) x = Math.pow(x, parseFactor()); // exponentiation\par
\par
                return x;\par
            \}\par
        \}.parse();\par
    \}\par
